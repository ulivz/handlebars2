{
  "name": "handlebars2",
  "version": "0.0.16",
  "description": "A simple wrapper of handlebars",
  "main": "dist/handlebars2.common.js",
  "module": "dist/my-package.esm.js",
  "dependencies": {
    "handlebars": "^4.0.10",
    "handlebars-utils": "^1.0.4"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "bili": "^0.18.1",
    "conventional-changelog-cli": "^1.3.3",
    "eslint-config-rem": "^3.2.0",
    "jest-cli": "^21.2.1",
    "xo": "^0.18.2"
  },
  "files": [
    "dist/*.js",
    "lib",
    "test",
    "index.js"
  ],
  "scripts": {
    "build": "bili",
    "test": "BABEL_ENV=test jest --env node && npm run lint",
    "test:cov": "BABEL_ENV=test jest --env node --coverage && npm run lint",
    "lint": "xo src/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "ulivz",
  "license": "MIT",
  "jest": {
    "coverageDirectory": "./coverage/"
  },
  "xo": {
    "extends": "rem/prettier",
    "envs": [
      "jest"
    ],
    "rules": {
      "import/no-unresolved": 0,
      "valid-typeof": 0,
      "import/no-mutable-exports": 0
    },
    "ignores": [
      "docs/**",
      "src/helpers/handlebars-utils.js"
    ]
  }
}
