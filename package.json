{
  "name": "handlebars2",
  "version": "0.0.13",
  "description": "A simple wrapper of handlebars",
  "main": "dist/handlebars2.js",
  "module": "dist/my-package.esm.js",
  "dependencies": {
    "handlebars": "^4.0.10",
    "handlebars-helpers": "^0.9.8"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "bili": "^0.18.1",
    "conventional-changelog-cli": "^1.3.3",
    "jest-cli": "^21.2.1"
  },
  "files": [
    "dist/*.js",
    "lib",
    "index.js"
  ],
  "scripts": {
    "build": "bili",
    "test": "BABEL_ENV=test jest --env node && npm run lint && npm run build --no-cache",
    "test:cov": "BABEL_ENV=test jest --env node --coverage && xo src/*.js",
    "lint": "xo",
    "lint:fix": "xo --fix"
  },
  "author": "ulivz",
  "license": "MIT",
  "jest": {
    "coverageDirectory": "./coverage/"
  },
  "xo": {
    "extends": "rem/prettier",
    "envs": [
      "jest"
    ],
    "rules": {
      "import/no-unresolved": 0
    },
    "ignores": [
      "docs/**"
    ]
  }
}
